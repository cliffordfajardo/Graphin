{
  "name": "@antv/graphin",
  "version": "2.5.0",
  "description": "the react toolkit for graph analysis based on g6",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run clean && father build --watch",
    "build": "npm run clean && father build && npm run build:umd",
    "build:umd": "node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack.js  --mode production  -c ./webpack.config.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "clean": "rimraf es esm lib dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/Graphin.git",
    "directory": "packages/graphin"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@antv/g-base": "^0.5.9",
    "@antv/hierarchy": "^0.6.8",
    "@babel/core": "^7.17.5",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/d3-quadtree": "^3.0.2",
    "@types/jest": "^27.4.1",
    "@types/lodash-es": "^4.17.6",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eventemitter3": "^4.0.7",
    "father": "^2.30.13",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "jsdom-worker": "^0.1.0",
    "less": "^3.8.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "svg-path-parser": "^1.1.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "sideEffects": [
    "*.css"
  ],
  "author": "AntV",
  "license": "MIT",
  "dependencies": {
    "@antv/util": "^2.0.17",
    "@types/lodash.clonedeep": "^4.5.6",
    "d3-quadtree": "^3.0.1",
    "lodash.clonedeep": "^4.5.0",
    "@antv/g6": "^4.6.0-beta.3"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
